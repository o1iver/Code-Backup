    minor=`echo $version | cut -d'.' -f2`
    micro=`echo $version | cut -d'.' -f3`
    beta=`echo $version | cut -d'.' -f4`
    if test -z $beta; then
      beta=0
    fi
    SWFMILL_VERSION=`printf %2.2d%2.2d%2.2d%2.2d $major $minor $micro $beta`
  fi

